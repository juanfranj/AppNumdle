<Numdle>
    FloatLayout:
        padding: "5dp", "5dp"
        #spacing: "5dp", "5dp"

        canvas:
            
            Color:
                rgba: 251/255, 252/255, 252/255
            Rectangle:
                pos: self.pos
                size: self.size

        # MDLabel:
        #     pos_hint: {'right': 1,'top': 1}
        #     size_hint: (.2, 0.05)
        #     text: "numdle."
        #     font_style: "H6"
        
        GridLayout:
            id:grid
            padding: "1dp", "1dp"
            spacing: "4dp", "4dp"
            pos_hint:{'x':0.025, 'top': .99}
            size_hint: (.95, .4)
            rows: 6
            cols: 8
            #row_force_default: True
            #row_default_height: "40dp"
            #height: self.minimum_height

            

        GridLayout:
            id:teclado
            padding: "1dp", "1dp"
            spacing: "4dp", "4dp"
            pos_hint:{'x':0.025, 'top': 0.58}
            size_hint: (.95, .06)
            rows: 1
            cols: 10
            # row_force_default: True
            # row_default_height: "40dp"
            
        GridLayout:
            id:simbolos
            padding: "3dp", "3dp"
            spacing: "4dp", "4dp"
            pos_hint:{'x':0.05, 'top': 0.52}
            size_hint: (.9, .06)
            rows: 1
            cols: 7
            # row_force_default: True
            # row_default_height: "40dp"
        
        BoxLayout:
            id:error
            padding: "1dp", "1dp"
            pos_hint:{'x':0.025, 'top': 2}
            size_hint: (.95, .1)
            canvas:
                Color:
                    rgba: 245/255, 183/255, 177/255, 1
                RoundedRectangle:
                    pos: self.pos
                    size: self.size
                    radius: [10]
            Label:
                id: mensaje
                text: "Prueba"
                halign: "center"
                bold: True
                font_style: "H6"
                font_size: "18sp"
                color: 0, 0, 0, 1            
        
        