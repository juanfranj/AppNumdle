<Numdle>
    FloatLayout:
        padding: "5dp", "5dp"
        #spacing: "5dp", "5dp"

        canvas:
            
            Color:
                rgba: 251/255, 252/255, 252/255
            Rectangle:
                pos: self.pos
                size: self.size

        # MDLabel:
        #     pos_hint: {'right': 1,'top': 1}
        #     size_hint: (.2, 0.05)
        #     text: "numdle."
        #     font_style: "H6"
        
        GridLayout:
            id:grid
            padding: "5dp", "5dp"
            spacing: "4dp", "4dp"
            pos_hint:{'x':0.05, 'top': .98}
            size_hint: (.9, .5)
            rows: 6
            cols: 8
            row_force_default: True
            row_default_height: "40dp"
            

        GridLayout:
            id:teclado
            padding: "2dp", "2dp"
            spacing: "4dp", "4dp"
            pos_hint:{'x':0.05, 'top': 0.55}
            size_hint: (.9, .2)
            rows: 1
            cols: 10
            row_force_default: True
            row_default_height: "40dp"
            
        GridLayout:
            id:simbolos
            padding: "3dp", "3dp"
            spacing: "4dp", "4dp"
            pos_hint:{'x':0.08, 'top': 0.48}
            size_hint: (.84, .2)
            rows: 1
            cols: 7
            row_force_default: True
            row_default_height: "40dp"
            
        
        